package com.nargiza;

public class Main {

    private static int addition (int num1, int num2){
        return num1+num2;
    }

    private static int subtraction (int num1, int num2){
        return num1-num2;
    }

    private static int multiplication (int num1, int num2){
        return num1*num2;
    }

    private static double division (int num1, int num2){
        return (double)num1/num2;

    }
    public static void main(String[] args) {
        System.out.println(addition(5,7));
        System.out.println(subtraction(5,7));
        System.out.println(division(5,7));
        System.out.println(multiplication(5,7));

    }
}
