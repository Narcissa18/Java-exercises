package com.nargiza;


public class Main {
    static int findMax (int n1, int n2) {
        if (n1>n2)
    return n1;
    return n2;
    }
    static int findMax (int n1, int n2, int n3) {
        if (n1>n2)
            return n1;
        else if (n2>n3)
            return n2;
        return n3;
    }
    public static void main(String[] args) {
        System.out.println(findMax(5,8));
        System.out.println(findMax(3,7,9));


    }
}
