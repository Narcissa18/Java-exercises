package com.nargiza;

import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);
        System.out.println("Please enter a number");
        int num= input.nextInt();
         for (int i =1; i<=10;i++){
             System.out.println(i+"*"+num+"="+i*num);
         }

    }
}
